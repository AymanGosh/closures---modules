 const setCounter = function (num) {
  let counter = 0

  const count = function () {
    counter += num;
    console.log(counter)
  }

  return count
}

const increment = setCounter(2)
increment()
/////////////////
const Family = function () {
    const members = []

    const birth = function (name) {
        members.push(name)
        console.log(members)
    }

    return birth
}

const giveBirth = Family()
giveBirth("Clarissa")
giveBirth("Mayana")
